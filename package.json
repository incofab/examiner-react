    "electron": "^35.0.1",
    "electron-builder": "^25.1.8",
    "env-cmd": "^10.1.0",
    "react-scripts": "^5.0.1",
    "typescript": "^4.9.5",
    "wait-on": "^8.0.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
